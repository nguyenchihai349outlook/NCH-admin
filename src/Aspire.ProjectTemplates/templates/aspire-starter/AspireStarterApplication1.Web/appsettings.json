{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  //#if (UseRedisCache)
  "Aspire": {
    "StackExchange:Redis": {
      "ConfigurationOptions": {
        "AbortOnConnectFail": false,
        "ConnectRetry": 2
      }
    }
  },
  //#endif
  "AllowedHosts": "*"
}
